<div class="login-container">
  <div id="fc-login">
    <h2>Log in to vrMIS3</h2>

    <form class="login-form">
      <div class="form-errors">Invalid access code</div>
      <div class="field">
        <label for="user-access_code">Enter access code</label>
        <input type="password" name="access_code" id="user-access_code" />
      </div>

      <div>
        <button class="login" id="user-login">Log in</button>
      </div>
    </form>
  </div>
</div>
